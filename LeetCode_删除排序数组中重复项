//我的作弊式解法：利用std::unique  
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        auto result=unique(nums.begin(),nums.end());
        nums.erase(result,nums.end());
        return nums.size();
    }
};

//别人家的解法：count记录重复元素的个数，当发现不一样时精准插入
int count = 0;
for(int i = 1; i < n; i++){
    if(A[i] == A[i-1]) count++;
    else A[i-count] = A[i];
}
return nums.size()-count;
